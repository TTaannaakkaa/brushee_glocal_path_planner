<?xml version="1.0"?>
<launch>
    <!--  config file path  -->
    <arg name="local_map_creator_settings" default="$(find brushee_local_map_creator)/config/param/bruheee_local_map_creator.yaml"/>
    <arg name="obstacle_detector_settings" default="$(find bruhsee_obstacle_detector)/config/param/bruhsee_obstacle_detector.yaml"/>
   
    <!--  node launch  -->
    <node name="brushee_local_map_creator" pkg="bruhsee_local_map_creator" type="brushee_local_map_creator_node" output="screen">
        <rosparam command="load" file="$(arg local_map_creator_settings)"/>
    </node>
    <node name="bruhsee_obstacle_detector" pkg="bruhsee_obstacle_detector" type="brushee_obstacle_detector_node" output="screen">
        <rosparam command="load" file="$(arg obstacle_detector_settings)"/>
    </node>
</launch>